{
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/favicon-io/favicon-16x16.png",
      "32": "/images/favicon-io/favicon-32x32.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["content-script.js"]
    }
  ],
  "description": "Find the search volume for your keywords",
  "icons": {
    "16": "/images/favicon-io/favicon-16x16.png",
    "32": "/images/favicon-io/favicon-32x32.png"
  },
  "manifest_version": 3,
  "name": "keyword-search-volume",
  "permissions": ["storage"],
  "version": "0.0.2"
}
